{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"redirects": [
		{
			"source": "/",
			"destination": "https://svelte.jp/",
			"permanent": true
		},
		{
			"source": "/docs/layouts",
			"destination": "https://svelte.jp/docs/kit/routing",
			"permanent": true
		},
		{
			"source": "/docs/loading",
			"destination": "https://svelte.jp/docs/kit/load",
			"permanent": true
		},
		{
			"source": "/docs/types",
			"destination": "https://svelte.jp/docs/kit/@sveltejs-kit"
		},
		{
			"source": "/docs",
			"destination": "https://svelte.jp/docs/kit",
			"permanent": true
		},
		{
			"source": "/docs/(.*)",
			"destination": "https://svelte.jp/docs/kit/$1",
			"permanent": true
		},
		{
			"source": "/(.*)",
			"destination": "https://svelte.jp"
		}
	]
}
